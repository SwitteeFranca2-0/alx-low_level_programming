#include "main.h"
#include <stdio.h>

int main(void)
{
	char *s = "hello, world";
	char *f = "world";
	char *t;

	t = _strpbrk(s, f);
	printf("%s\n", t);
	return (0);
}

char *_strpbrk(char *s, char *accept)
{
	int i;
	int b;

	i = 0;
	while (s[i])
	{
		for (b = 0; accept[b] != '\0'; b++)
		{
			if (s[i] == accept[b])
			{
				s += i;
				return (s);
			}
		}
		i++;
	}
	return ('\0');
}

