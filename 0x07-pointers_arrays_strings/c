#include "main.h"
#include <stdio.h>

int main(void)
{
	char *s = "hello, world2";
	char *f = "oleh";
	unsigned int n;

	n = _strspn(s, f);
	printf("%u\n", n);
	return (0);
}

unsigned int _strspn(char *s, char *accept)
{
	int i, b, m;

	i = 0;
	m = 0;

	while ((s[i] >= 97 && s[i] <= 122) || (s[i] >= 65 && s[i] <= 90) && s[i])
	{
		for (b = 0; accept[b] != '\0'; b++)
		{
			if (s[i] == accept[b])
				m++;
		}
		i++;
	}
	return (m);
}
