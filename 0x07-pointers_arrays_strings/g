#include <stdio.h>
#include "main.h"

/**
 * print_diagsums - print
 * @a: a
 * @size: size
 * Return: none.
 */

int main(void)
{
	int c[3][3] = {
		{0, 1, 5},
		{10, 11, 12},
		{1000, 101, 102},
	};
	
	print_diagsums((int *)c, 3);
	return(0);
}

void print_diagsums(int *a, int size)
{
	int b, n, sum, sec_sum;

	sum = 0;
	sec_sum = 0;

	for (b = 0; b < size; b++)
	{
		sum += a[b][b];
		sec_sum += a[b][size - 1- b];
	}

	printf("%d, %d\n", sum, sec_sum);
}

